{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": "standard",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": "off",
		"eqeqeq": "warn",
		"max-len": [2, { "code": 800 }]
	}
}
